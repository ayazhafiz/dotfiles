### Data

# ENV
export GOPATH="$HOME/gocode"
export GOROOT="/usr/local/Cellar/go/1.12.4/libexec/"
export PATH="$PATH:/usr/bin:$GOPATH/bin:$HOME/.rvm/bin:$HOME/.cargo/bin"
export EDITOR="nvim"
export RUST_SRC_PATH="$(rustc --print sysroot)/lib/rustlib/src/rust/src"

# Aliases
alias alacritty='/Applications/Alacritty.app/Contents/MacOS/alacritty'
alias config='/usr/bin/git --git-dir=$HOME/.cfg/ --work-tree=$HOME'
alias vim="nvim"
alias lldb="PATH=/usr/bin:$PATH lldb" # source MacOS python
alias python="python3"
alias pip="pip3"
alias xs="exa -lhga"

V() { cd ~/Desktop/Vanderbilt/$1 }
ranger() {
    if [ -z "$RANGER_LEVEL" ]; then
	/usr/local/bin/ranger "$@"
    else
        exit
    fi
}

### Customization

# Prompt Autoload
autoload -U promptinit; promptinit

# Pure
PURE_GIT_PULL=1
#prompt pure

# Pure Power (Powerlevel10k)
POWERLEVEL9K_IGNORE_TERM_COLORS=true
source ~/.purepower

# Syntax Highlighting
source /usr/local/share/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh
